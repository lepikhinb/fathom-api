{
    "name": "based/fathom",
    "description": "This is my package Fathom",
    "keywords": [
        "based",
        "laravel",
        "fathom"
    ],
    "homepage": "https://github.com/based/fathom",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Lepikhin",
            "email": "boris@lepikhin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Based\\Fathom\\": "src",
            "Based\\Fathom\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Based\\Fathom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Based\\Fathom\\FathomServiceProvider"
            ],
            "aliases": {
                "Fathom": "Based\\Fathom\\FathomFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
