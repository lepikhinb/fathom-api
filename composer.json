{
    "name": "based/fathom",
    "description": "This is my package Fathom",
    "keywords": [
        "based",
        "laravel",
        "fathom"
    ],
    "homepage": "https://github.com/lepikhinb/fathom-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Lepikhin",
            "email": "boris@lepikhin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/http": "^8.54",
        "orchestra/testbench": "^6.20",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.6.10",
        "pestphp/pest": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "Based\\Fathom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Based\\Fathom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "stan": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Based\\Fathom\\FathomServiceProvider"
            ],
            "aliases": {
                "Fathom": "Based\\Fathom\\Facade\\Fathom"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}